{
  "name": "@salt-ds/icons",
  "version": "1.17.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpmorganchase/salt-ds.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/jpmorganchase/salt-ds/issues",
  "main": "src/index.ts",
  "files": [
    "css",
    "saltIcons.css"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:icons && yarn node ../../scripts/build.mjs",
    "build:icons": "yarn node ./scripts/generateIcons.mjs '*.svg'",
    "copy:css": "yarn node ./scripts/copyCss.mjs",
    "clean": "rimraf ./src/components"
  },
  "dependencies": {
    "@salt-ds/styles": "workspace:*",
    "@salt-ds/window": "workspace:*",
    "clsx": "^2.0.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.0",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "glob": "^10.0.0",
    "mustache": "^4.2.0",
    "rimraf": "^5.0.0",
    "svgo": "^4.0.0"
  },
  "publishConfig": {
    "directory": "../../dist/salt-ds-icons",
    "provenance": true
  }
}

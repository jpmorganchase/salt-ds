{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":preserveSemverRanges"],
  "postUpdateOptions": ["yarnDedupeHighest"],
  "packageRules": [
    {
      "groupName": "Mosaic",
      "matchPackagePrefixes": ["@jpmorganchase/mosaic"]
    },
    {
      "groupName": "AG Grid",
      "matchPackagePrefixes": ["@ag-grid"]
    }
  ]
}

{
  "name": "@salt-ds/mosaic-site",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "description": "@salt-ds/mosaic-site Site generated by Mosaic generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "clean": "rm -fr public/.tmp .next",
    "build": "next build",
    "dev": "next dev",
    "postinstall": "patch-package",
    "start": "next start",
    "gen:snapshot": "yarn mosaic build --out snapshots --name latest --config mosaic.config.js",
    "serve:snapshot:file": "yarn cross-env MOSAIC_MODE='snapshot-file' concurrently --kill-others 'yarn dev'",
    "serve:snapshot:s3": "yarn cross-env MOSAIC_MODE='snapshot-s3' concurrently --kill-others 'yarn dev'",
    "serve": "concurrently --kill-others 'yarn dev' 'yarn mosaic serve -c ./mosaic.config.js' -p 8080"
  },
  "dependencies": {
    "@jpmorganchase/mosaic-cli": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-components": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-content-editor-plugin": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-core": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-layouts": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-plugins": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-schemas": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-serialisers": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-site-components": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-site-preset-styles": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-source-git-repo": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-source-local-folder": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-standard-generator": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-store": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-theme": "^0.1.0-beta.19",
    "@jpmorganchase/mosaic-types": "^0.1.0-beta.19",
    "@philpl/buble": "^0.19.7",
    "@types/react": "^18.0.0",
    "next": "^13.1.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.3.1",
    "@types/node": "^14.14.20",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.3.1",
    "patch-package": "^6.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}

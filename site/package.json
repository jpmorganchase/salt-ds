{
  "name": "@salt-ds/site",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "description": "@salt-ds/site Site generated by Mosaic generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "clean": "rm -fr public/.tmp .next",
    "build": "yarn prebuild:theme && yarn gen:css && next build",
    "prebuild:theme": "yarn workspace @salt-ds/ag-grid-theme build && yarn workspace @salt-ds/react-resizable-panels-theme build && yarn workspace @salt-ds/highcharts-theme build",
    "deploy": "yarn mosaic deploy --out snapshots --name latest --config mosaic.config.js",
    "dev": "next dev",
    "start": "next start",
    "gen:snapshot": "yarn gen:props && yarn gen:css && yarn mosaic build --out snapshots --name latest --config mosaic.config.js",
    "serve:snapshot:file": "yarn cross-env MOSAIC_MODE='snapshot-file' concurrently --kill-others 'yarn dev'",
    "serve:snapshot:s3": "yarn cross-env MOSAIC_MODE='snapshot-s3' concurrently --kill-others 'yarn dev'",
    "serve:mosaic": "yarn mosaic serve -c ./mosaic.config.js -p 8080",
    "serve": "concurrently --kill-others 'yarn dev' 'yarn serve:mosaic'",
    "gen:props": "yarn node './propsGen.js'",
    "gen:css": "yarn node './cssGen.mjs'"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@jpmorganchase/mosaic-cli": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-layouts": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-plugins": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-schemas": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-source-git-repo": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-source-http": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-source-local-folder": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-standard-generator": "0.1.0-beta.92",
    "@jpmorganchase/mosaic-store": "0.1.0-beta.92",
    "css-tree": "^3.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.6.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.6.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "next": "^14.2.26",
    "next-auth": "^4.24.11",
    "shiki": "^3.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.22",
    "concurrently": "^9.0.0",
    "cross-env": "^10.0.0",
    "dotenv-load": "^2.0.1",
    "embla-carousel-class-names": "^8.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^8.0.7"
  }
}
